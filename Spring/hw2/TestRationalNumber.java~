public class TestRationalNumber {   
   public static void main( String[] args )    {      
      RationalNumber x;     
      RationalNumber y;      
      RationalNumber z;   
      
      x = new RationalNumber( 2, 20 );    
      y = new RationalNumber( -3, 30 );    
      
      z = x.add( y );       
      System.out.println( x + " + " + y " = " + z );       
      z = x.multiply( y );      
      System.out.println( x + " x " + y " = " + z );        
   } 
}
